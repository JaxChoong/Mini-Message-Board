<html>
    <head>
        <link rel="stylesheet" href="./styles.css">
        <title><%= title %></title>
    </head>
    <body>
        <div id="board">
            <% messages.forEach((message)=>{ %>
            <div class="paper">
                    <h3>Name:</h3><%= message.user %>
                    <br>
                    <h3>Message:</h3><%= message.text %>
                    <br>
                    <h3>Added:</h3><%= message.added %>
                    <br>
            </div>
            <% }) %>
        </div>
    </body>
</html>